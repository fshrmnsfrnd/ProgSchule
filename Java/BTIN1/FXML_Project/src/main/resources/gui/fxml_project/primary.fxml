<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/24" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.fxml_project.PrimaryController">
   <children>
      <Label text="Primary View" />
      <Button fx:id="btn2" mnemonicParsing="false" onAction="#btn2Click" text="Button" />
      <Button fx:id="primaryButton" onAction="#btnClick" text="Switch to Secondary View" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
